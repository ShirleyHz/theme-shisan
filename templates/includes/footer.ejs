	<footer class="site-footer u-textAlignCenter">
    	<div class="footer-branding">
            <a href="<%= themeConfig.domain %>" title="<%= themeConfig.siteName %>"><%= themeConfig.siteName %></a>
									<% if (site.customConfig.footerqm) { %><p class="site-description"><%= site.customConfig.footerqm%></p><% } %>
			         </div>
		 <hr />
		 <div class="social-links">
		                       <% ['rss', 'twitter', 'github', 'zhihu', 'facebook','linkedin','telegram','douban','instagram','qq','weibo','youtube'].forEach((item) => { %>
                            <% if (site.customConfig[item]) { %>	
			<span class="social-link"><a href="<%= site.customConfig[item] %>" target="_blank"><i class="iconfont icon-<%= item %>"></i></a></span>	
			                         <% } %>
                           <% }) %>
				</div>
        <div class="footer-copy">
                <%- themeConfig.footerInfo %>
        </div>

    </footer>
	<div class="back-to-top u-hide" onclick="backToTop();"><i class="iconfont icon-xiangshang"></i></div>

<script type='text/javascript' src='<%= themeConfig.domain %>/media/scripts/main.js'></script>